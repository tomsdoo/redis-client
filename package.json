{
  "name": "@tomsd/redis-client",
  "version": "1.2.0",
  "description": "It's a wrapper of ioredis.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "biome check",
    "prepare": "husky",
    "start-redis": "docker compose up -d",
    "stop-redis": "docker compose down",
    "serve:doc": "mdbook --serve --directory docs",
    "test": "vitest --project node",
    "test:local": "vitest --project local"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint"
    ],
    "__test__/**/*.ts": [
      "npm run lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsdoo/redis-client.git"
  },
  "keywords": [
    "redis"
  ],
  "author": "tom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomsdoo/redis-client/issues"
  },
  "homepage": "https://github.com/tomsdoo/redis-client#readme",
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@tomsd/md-book": "2.0.1",
    "@types/node": "24.2.0",
    "@types/uuid": "10.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.1.4",
    "ts-node": "10.9.2",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "vitest": "3.2.4"
  },
  "dependencies": {
    "ioredis": "5.6.1",
    "uuid": "11.1.0"
  }
}
