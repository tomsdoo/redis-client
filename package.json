{
  "name": "@tomsd/redis-client",
  "version": "1.0.3",
  "description": "It's a wrapper of ioredis.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json",
    "lint": "biome check",
    "prepare": "husky",
    "start-redis": "docker run --rm -d -p 6379:6379 --name redis-instance redis:alpine",
    "stop-redis": "docker container stop redis-instance",
    "serve:doc": "mdbook --serve --directory docs",
    "test": "vitest"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint"
    ],
    "__test__/**/*.ts": [
      "npm run lint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomsdoo/redis-client.git"
  },
  "keywords": [
    "redis"
  ],
  "author": "tom",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tomsdoo/redis-client/issues"
  },
  "homepage": "https://github.com/tomsdoo/redis-client#readme",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tomsd/md-book": "1.3.3",
    "@types/node": "22.10.1",
    "@types/uuid": "10.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "vitest": "2.1.6"
  },
  "dependencies": {
    "ioredis": "5.4.1",
    "uuid": "11.0.3"
  }
}
